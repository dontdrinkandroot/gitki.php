#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Dontdrinkandroot\Gitki\WebBundle\Command\ChangeConfigCommand;
use Dontdrinkandroot\Gitki\WebBundle\Command\CheckConfigCommand;
use Symfony\Component\Console\Application;

$application = new Application();
$application->add(new CheckConfigCommand());
$application->add(new ChangeConfigCommand());
$application->run();