#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Dontdrinkandroot\Gitki\WebBundle\Command\ConfigChangeCommand;
use Dontdrinkandroot\Gitki\WebBundle\Command\ConfigCheckCommand;
use Symfony\Component\Console\Application;

$application = new Application();
$application->add(new ConfigCheckCommand());
$application->add(new ConfigChangeCommand());
$application->run();